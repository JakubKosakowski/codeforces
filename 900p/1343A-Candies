#include <iostream>
#include <cmath>

using namespace std;


int main() {
	
	int t;

	cin >> t;

	while (t--) {
		long long x;

		int n = 3, k = 2;

		cin >> x;

		while (true) {
			if (x % n == 0) {
				cout << x / n << endl;
				break;
			}
			else {
				n += pow(2, k);
				++k;
			}
		}

	}

	return 0;
}
